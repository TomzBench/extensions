  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "rolldown -c --watch",
    "build": "rolldown -c && tsc --emitDeclarationOnly",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "rolldown": "1.0.0-beta.51",
    "typescript": "^5.7.2"
  }
}
